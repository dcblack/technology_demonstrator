#!/usr/bin/perl

my $reverse = 0;
if ($ARGV[0] eq '-r') {
  shift @ARGV;
  $reverse = 1;
}#endif
my @in = @ARGV;
my @out;
my %unique;
@in = reverse @in if $reverse;
for my $elt (@in) {
  next if exists $unique{$elt};
  $unique{$elt} = 1;
  push @out,$elt;
}#endfor
@out = reverse @out if $reverse;
print join("\n",@out),"\n";

